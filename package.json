{
	"author": {
		"name": "Baptiste Pillot",
		"email": "baptiste@pillot.fr"
	},
	"dependencies": {
		"@itrocks/class-type": "latest",
		"@itrocks/decorator": "latest",
		"typescript": "~5.6"
	},
	"description": "Manage and query composite and component property structures",
	"devDependencies": {
		"@types/node": "^22.10"
	},
	"engines": {
		"node": ">=18"
	},
	"exports": {
		"import": "./esm/composition.js",
		"require": "./cjs/composition.js"
	},
	"files": [
		"LICENSE",
		"README.md",
		"*/*.d.ts",
		"*/*.js",
		"!esm.*"
	],
	"homepage": "https://it.rocks",
	"keywords": [
		"backend",
		"component",
		"composite",
		"composition",
		"decorator",
		"it.rocks",
		"property"
	],
	"license": "ISC",
	"name": "@itrocks/composition",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/itrocks-ts/composition.git"
	},
	"scripts": {
		"build": "npm run build:cjs && npm run build:esm",
		"build:cjs": "npm run pre:cjs && tsc -p tsconfig.cjs.json",
		"build:esm": "npm run pre:esm && tsc -p tsconfig.esm.json && node esm/esm && npm run pre:cjs",
		"pre:cjs": "sed -i 's/\"type\": \"module\"/\"type\": \"commonjs\"/' package.json",
		"pre:esm": "sed -i 's/\"type\": \"commonjs\"/\"type\": \"module\"/' package.json"
	},
	"type": "commonjs",
	"types": "./esm/composition.d.ts",
	"version": "0.0.1"
}
